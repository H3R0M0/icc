{% for k,v in params|default({}) %}
    {% if v is not null %}
        {% if v is iterable %}
            {% for value in v %}
                <input type="hidden" name="{{ k }}[]" value="{{ value }}">
            {% endfor %}
        {% else %}
            <input type="hidden" name="{{ k }}" value="{{ v }}">
        {% endif %}
    {% endif %}
{% endfor %}