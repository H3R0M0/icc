{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.3.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "acelaya/doctrine-enum-type": "^2.2",
        "antishov/doctrine-extensions-bundle": "^1.4",
        "beberlei/doctrineextensions": "^1.2",
        "bentools/webpush-bundle": "0.6.*",
        "doctrine/doctrine-bundle": "2.0",
        "doctrine/doctrine-migrations-bundle": "^3.0.0",
        "easycorp/easyadmin-bundle": "^2.3",
        "emojione/emojione": "2.0",
        "frostiede/enum-bundle": "^3.0.0",
        "fzaninotto/faker": "^1.8",
        "javiereguiluz/easyslugger": "^1.0",
        "jms/serializer-bundle": "^3.2",
        "jsvrcek/ics": "^0.6.0",
        "knplabs/knp-menu-bundle": "^3.0",
        "league/commonmark": "^1.0",
        "league/csv": "^9.2",
        "lightsaml/sp-bundle": "^1.2",
        "myclabs/php-enum": "^1.6",
        "nelmio/api-doc-bundle": "^3.4",
        "nyholm/psr7": "^1.2",
        "oneup/flysystem-bundle": "^3.0",
        "ralouphie/mimey": "^2.1",
        "ramsey/uuid-doctrine": "^1.6",
        "schulit/common-bundle": "^1.0.0",
        "sensio/framework-extra-bundle": "^5.1",
        "sensiolabs/ansi-to-html": "^1.2",
        "shapecode/cron-bundle": "^4.0",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^4.4",
        "symfony/console": "^4.4",
        "symfony/dotenv": "^4.4",
        "symfony/error-handler": "^4.4",
        "symfony/expression-language": "^4.4",
        "symfony/flex": "^1.1",
        "symfony/form": "^4.4",
        "symfony/framework-bundle": "^4.4",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "^4.4",
        "symfony/security-bundle": "^4.4",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^4.4",
        "symfony/twig-bundle": "^4.4",
        "symfony/validator": "^4.4",
        "symfony/web-link": "^4.4",
        "symfony/webpack-encore-bundle": "^1.4",
        "symfony/yaml": "^4.4",
        "trikoder/oauth2-bundle": "^3.1",
        "vich/uploader-bundle": "^1.9"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.0",
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "league/flysystem-memory": "^1.0",
        "phpstan/phpstan": "^0.12.0",
        "phpstan/phpstan-doctrine": "^0.12.0",
        "phpstan/phpstan-symfony": "^0.12.0",
        "phpunit/phpunit": "^7.0",
        "psr/event-dispatcher": "^1.0",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^4.4"
        }
    }
}
