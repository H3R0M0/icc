{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "antishov/doctrine-extensions-bundle": "^1.4",
        "beberlei/doctrineextensions": "^1.2",
        "damienharper/auditor-bundle": "^3.4",
        "doctrine/doctrine-bundle": "2.3.*",
        "doctrine/doctrine-migrations-bundle": "^3.0.0",
        "easycorp/easyadmin-bundle": "^2.3",
        "eightpoints/guzzle-bundle": "^8.2",
        "fakerphp/faker": "^1.8",
        "frostiede/enum-bundle": "dev-master",
        "frostiede/webpush-bundle": "^0.7.0",
        "javiereguiluz/easyslugger": "^1.0",
        "jms/serializer-bundle": "^3.2",
        "jsvrcek/ics": "^0.8",
        "knplabs/knp-menu-bundle": "^3.0",
        "league/commonmark": "^1.0",
        "league/csv": "^9.2",
        "lightsaml2/lightsaml": "dev-master",
        "lightsaml2/sp-bundle": "dev-master",
        "lightsaml2/symfony-bridge": "dev-master",
        "myclabs/php-enum": "^1.6",
        "nelmio/api-doc-bundle": "^4.2",
        "nyholm/psr7": "^1.2",
        "oneup/flysystem-bundle": "^3.0",
        "ramsey/uuid-doctrine": "^1.6",
        "schulit/common-bundle": "^1.0.0",
        "sensio/framework-extra-bundle": "^5.1",
        "sensiolabs/ansi-to-html": "^1.2",
        "shapecode/cron-bundle": "^4.0",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^5.3",
        "symfony/console": "^5.3",
        "symfony/dotenv": "^5.3",
        "symfony/error-handler": "^5.3",
        "symfony/expression-language": "^5.3",
        "symfony/flex": "^1.1",
        "symfony/form": "^5.3",
        "symfony/framework-bundle": "^5.3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "^5.3",
        "symfony/security-bundle": "^5.3",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^5.3",
        "symfony/twig-bundle": "^5.3",
        "symfony/validator": "^5.3",
        "symfony/web-link": "^5.3",
        "symfony/webpack-encore-bundle": "^1.4",
        "symfony/yaml": "^5.3",
        "trikoder/oauth2-bundle": "^3.1",
        "vich/uploader-bundle": "^1.9",
        "willdurand/js-translation-bundle": "^4.0",
        "xantios/mimey": "^2.1"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.0",
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "league/flysystem-memory": "^1.0",
        "phpstan/phpstan": "^0.12.0",
        "phpstan/phpstan-doctrine": "^0.12.0",
        "phpstan/phpstan-symfony": "^0.12.0",
        "phpunit/phpunit": "^7.0",
        "psr/event-dispatcher": "^1.0",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.3"
        }
    }
}
