{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-openssl": "*",
        "acelaya/doctrine-enum-type": "^2.2",
        "emojione/emojione": "2.0",
        "javiereguiluz/easyslugger": "^1.0",
        "jms/serializer-bundle": "^3.2",
        "knplabs/knp-menu-bundle": "^2.2",
        "league/commonmark": "^0.18.4",
        "league/csv": "^9.2",
        "lightsaml/sp-bundle": "^1.2",
        "myclabs/php-enum": "^1.6",
        "nelmio/api-doc-bundle": "^3.4",
        "oneup/flysystem-bundle": "^3.0",
        "schoolit/common-bundle": "^1.2",
        "sensio/framework-extra-bundle": "^5.1",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/asset": "^4.2",
        "symfony/console": "^4.2",
        "symfony/dotenv": "^4.2",
        "symfony/expression-language": "^4.2",
        "symfony/flex": "^1.1",
        "symfony/form": "^4.2",
        "symfony/framework-bundle": "^4.2",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "^4.2",
        "symfony/security-bundle": "^4.2",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^4.2",
        "symfony/twig-bundle": "^4.2",
        "symfony/validator": "^4.2",
        "symfony/web-link": "^4.2",
        "symfony/webpack-encore-bundle": "^1.4",
        "symfony/yaml": "^4.2",
        "vich/uploader-bundle": "^1.9",
        "webuni/commonmark-table-extension": "^0.9.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^5.0",
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "league/flysystem-memory": "^1.0",
        "phpstan/phpstan": "^0.11.4",
        "phpstan/phpstan-doctrine": "^0.11.2",
        "phpstan/phpstan-symfony": "^0.11.2",
        "phpunit/phpunit": "^7.0",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "^4.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^4.2"
        }
    }
}
